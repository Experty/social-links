{
  "name": "social-links",
  "version": "0.0.0",
  "description": "Validate & sanitize social links",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "nodemon --ignore './lib' -e ts,json --exec 'npm run build && npm run test && true'",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./lib",
    "prepare": "npm run build",
    "test": "jest --no-cache ./*",
    "test:watch": "jest --no-cache --watch ./*"
  },
  "keywords": ["social", "links", "media", "social-links", "social-media", "validate", "validation", "is-valid", "sanitize", "sanitization", "linkedin", "twitter", "facebook", "youtube", "twitch", "discord", "instagram", "patreon"],
  "author": "Grzegorz Kucmierz",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
